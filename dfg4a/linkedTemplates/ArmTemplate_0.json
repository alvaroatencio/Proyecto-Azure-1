		"triggerBlob_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/5b3af01b-6743-49a6-8e13-b4ad5760cdcb/resourceGroups/RgAlvaroatenciop95/providers/Microsoft.Storage/storageAccounts/storg4"
		}
	},
